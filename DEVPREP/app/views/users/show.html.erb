<%= render "prep_courses/header" %>
<!-- Full Width Image Header -->
<header class="header-image" style="background-image: url('<%= @user.cover.url %>'); background-size: cover">
  <div class="headline">
    <div class="container">
      <img id="prof-page-pic" src="<%= @user.avatar.url %>">
      <h1>I AM <%= @user.first_name %></h1>
      <h4><%= @user.title %></h4>
    </div>
  </div>
</header>

<!-- Page Content -->
<!-- Page Content -->
<div class="container">

  <hr>

  <div class="row">
    <div class="col-sm-8">
      <h2>Who Is <%= @user.first_name %>?</h2>
      <p><%= @user.bio %></p>
      <a href="/users/<%= @user.id %>/posts" id="blue-button" class="btn btn-lg btn-outline">
          <i class="fa fa-share"></i>Checkout My Articles
      </a>
    </p>
  </div>
  <div class="col-sm-4">
    <ul id="no-bullet">
      <li><a href="/conversations/"><img id="special-mail" id="mail-icon" src="/assets/message.png"/></a><h2>Hit Me Up</h2></li>
    </ul>
    <ul id="no-bullet">
      <a href="https://slack.com/<%= @user.slack %>"><li><img class="icon icons8-Slack" src="data:image/png;base64," width="40" height="40"></li><li class="move-up">@<%= @user.slack %></li></a>
    </ul>
    <hr>
    <ul id="no-bullet">
      <a target="_blank" href="https://twitter.com/<%= @user.twitter %>"><li><img class="icon icons8-Twitter" src="data:image/png;base64," width="45" height="45"></li><li class="move-up">@<%= @user.twitter %></li></a>
    </ul>
    <hr>
    <ul id="no-bullet">
      <a target="_blank" href="https://www.instagram.com/<%= @user.instagram %>/"><li><img class="icon icons8-Instagram" src="data:image/png;base64," width="45" height="45"></li><li class="move-up">@<%= @user.instagram %></li></a>
    </ul>
    <hr>
    <ul id="no-bullet">
      <a href="https://github.com/<%= @user.git %>"><li><img class="icon icons8-GitHub-Filled" src="data:image/png;base64," width="40" height="40"></li><li class="move-up">@<%= @user.git %></li></a>
    </ul>
  </div>
</div>
